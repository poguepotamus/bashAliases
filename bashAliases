  #bash -
    numInDir () {
      find "$1"/* -maxdepth 0 -type d | wc -l
    }
    numDrives () {
      numInDir $mntDir
    }
    checkVar=''
    setCheck () {
      checkVar=$1
    }
    check () {
      eval checking=\$$checkVar
      echo "$checking"
    }
    alias ck=check
    #drives -
      alias drives='sudo fdisk -l'
      mntDir='/home/alex/mnt'
      makeDriveDir='sdb1'
      makeDriveType='vfat'
      makeDriveName='makeDrive'
      tried='0'
      getmntInfo () {
        clear
        x=''
        mntDriveType='vfat'
        mntDriveDir='sdb1'
        mntDriveName='DefaultDriveName'
        drives
        if (("$tried"=='1')); then
          echo "You did not enter a valid value, please try again"
        fi
        read -p "Is the drive \`vfat/sdb1\`? " -n 1 x; while read -n 1 -t .01 y; do x="$x$y"; done
        echo; wait
        if [[ "$x" == y* ]] || [[ "$x" == n* ]]; then
          if [[ "$x" == y* ]]; then
            read -p "What do you want the drive to be named? " mntDriveName
          else
            read -p "What is the drive type? " mntDriveType
            read -p "What is the drive directory? " mntDriveDir
            read -p "What do you want the drive to be named? " mntDriveName
          fi
        else
          tried='1'
          mnt
        fi
        tried='0'
      }
      mntDrive () { #Needs drive type, name, and direcotry to mount. Can get with getmntInfo.
        sudo mkdir "$mntDir"/"$mntDriveName"
        sudo mount -t "$mntDriveType" /dev/"$mntDriveDir" "$mntDir"/"$mntDriveName"
        cd "$mntDir"/"$mntDriveName"
      }
      mnt () {
        getmntInfo
        wait
        mntDrive
      }
      getumntInfo () {
        numDrives=$(numInDir "$mntDir")
        #if [[ numDrives == 0 ]]; then
          #umntDriveFullDir='$(find "$mntDir"/* -maxdepth 0 type -d)'
        ls -l /home/alex/mnt
        read -p "Which drive would you like to unmount? " umntDriveName
      }
      umntDrive () {
        if [[ $(pwd) == "$mntDir"/"$umntDriveName"* ]]; then
          cd "$mntDir"
        fi
        sudo umount "$mntDir"/"$umntDriveName"
        sudo rmdir "$mntDir"/"$umntDriveName"
        sync
      }
      umnt () {
        getumntInfo
        wait
        umntDrive
      }
      umntDriveForce () {
        if [[ $(pwd) == "$mntDir"/"$umntDriveName"* ]]; then
          cd "$mntDir"
        fi
        sudo umount -l "$mntDir"/"$umntDriveName"
        sudo rmdir "$mntDir"/"$umntDriveName"
        sync
      }
      umntForce () {
        getumntInfo
        wait
        umntDriveForce
      }
      mdmnt () {
        mntDriveName=$makeDriveName
        mntDriveDir=$makeDriveDir
        mntDriveType=$makeDriveType
        mntDrive
      }
      umdmnt () {
        mntDriveName=$makeDriveName
        umntDrive
      }
      umdmntForce () {
        mntDriveName=$makeDriveName
        umntDriveForce
      }
