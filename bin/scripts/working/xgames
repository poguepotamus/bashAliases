#!/bin/bash
# T2.Lshell
# requirements -

# import -

# notes -

# code -
  # FUNCTIONS
  scriptHelp () {
    echo "
    "
  }

  if [[ $# -eq 0 ]]; then # If there are 0 arguments - i.e. only the script name
  fi
  while [[ $# -gt 0 ]]; do # While there are more than 0 arguments
    key="$1"
    case $key in
      -l|--list)

      ;;

      -h|--help)
        scriptHelp
      ;;

      *)
        scriptHelp
      ;;
    esac
    shift
  done
