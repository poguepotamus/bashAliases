# code -
  # testing -
    alias FTM="echo This is the First test message; wait; alias test=STM"
    alias STM="echo This is the Second test message; wait; alias test=FTM"
    alias test="FTM"
  # preferences -
    alias ll="ls -l --color"
    alias lall="ls -l -a --color"
    alias lal="lall --color"
    alias del="echo \"rm -rf\" "$1""
  # reload -
    bashAliasesFile="~/customBash/bashAliases"
    alias updateBash=". "$home"/customBash/bashAliases"
    alias updatebash="updateBash"
    alias bashreset="reset; wait; updateBash"
